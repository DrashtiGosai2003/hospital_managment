<?xml version="1.0"?>
<odoo>
       <record id="doctor_form" model="ir.ui.view">
        <field name="name">hospital.doctor.form</field>
        <field name="model">hospital.doctor</field>
        <field name="arch" type="xml">
            <form string="patient_view">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="specailization"/>
                        <field name="gender"/>
                        <field name="patient_ids"/>
                       
                    </group>
                </sheet>
            </form>
        </field>
       </record>

       <record id="view_doctor_list" model="ir.ui.view">
        <field name="name">hospital.doctor.list</field>
        <field name="model">hospital.doctor</field>
        <field name="arch" type="xml">
            <list string="Doctor">
                <field name="name"/>
                <field name="specailization"/>
                <field name="patient_ids"/>

            </list>
        </field>
    </record>

     <record id="doctor_record" model="ir.actions.act_window">
        <field name="name">Doctor Details</field>
        <field name="res_model">hospital.doctor</field>
        <field name="view_mode">list,form</field>
    </record>
    
     <menuitem
        id="model_name_menuu"
        name="Doctor"
        parent="patient_menu"
        action="doctor_record"
        sequence="20"/>
</odoo>